{
  "lock": "./deno.lock",
  "tasks": {
    "lint": "deno lint",
    "fmt": "deno fmt",
    "udd": "deno run -r --allow-read=. --allow-write=. --allow-net https://deno.land/x/udd/main.ts server/deps.ts && deno task lock",
    "lock": "deno cache --lock=deno.lock --lock-write server/deps.ts",
    "all": "deno task udd && deno task lint && deno task fmt",
    "serve": "deno run -A server/main.ts"
  }
}
